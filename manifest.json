{
  "manifest_version": 3,
  "name": "SubTranslate",
  "version": "1.0",
  "description": "Subtitles translation for language learning",
  "action": {
      "default_popup": "view/popup.html"
    },
    "permissions": ["activeTab"],
    "background": {
        "service_worker": "scripts/background.js"
    }
}